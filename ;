#include "fillit.h"
#include <stdio.h>

t_table	*place(t_tetra *tet, t_table *tab, int nb)
{
	int j = 0;
	ft_putnbr(nb);
	ft_putchar('\n');
	ft_memmove(tet->tab, tab->square, nb);
	ft_putnbr(nb);
	while (tab->square[j] != '\0')
	{
		printf("%c\n", tab->*square[j]);
	}
	return(tab);

}
